<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="540" prefWidth="640" stylesheets="@../css/config.css" xmlns="http://javafx.com/javafx/"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.alan344.controller.ConfigController">
    <center>
        <SplitPane fx:id="splitPane" dividerPositions="0.2">
            <BorderPane>
                <center>
                    <VBox fx:id="centerVBox" prefHeight="50.0" spacing="10.0">

                    </VBox>
                </center>
                <top>
                    <AnchorPane prefHeight="35.0" BorderPane.alignment="CENTER">
                        <children>
                            <Button styleClass="addConfigButton" layoutX="6.0" layoutY="3.0" mnemonicParsing="false"
                                    focusTraversable="false"
                                    text="+添加" onAction="#addEmptyExportPane"/>
                        </children>
                    </AnchorPane>
                </top>
            </BorderPane>
        </SplitPane>
    </center>
</BorderPane>
