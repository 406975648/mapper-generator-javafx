<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="450" prefWidth="610" stylesheets="@../css/config.css" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.alan344.controller.ConfigController">
    <center>
        <SplitPane fx:id="splitPane" dividerPositions="0.2">
            <items>
                <BorderPane >
                    <center>
                        <VBox fx:id="centerVBox" prefHeight="50.0" spacing="10.0">

                        </VBox>
                    </center>
                    <top>
                        <AnchorPane prefHeight="29.0" prefWidth="149.0" BorderPane.alignment="CENTER">
                            <children>
                                <Button styleClass="addConfigButton" layoutX="6.0" layoutY="3.0" mnemonicParsing="false" focusTraversable="false"
                                        text="+添加" onAction="#addEmptyExportPane"/>
                            </children>
                        </AnchorPane>
                    </top>
                </BorderPane>
            </items>
        </SplitPane>
    </center>
</BorderPane>
